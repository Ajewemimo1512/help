    <!-- Modal -->
    <div
      class="modal fade"
      id="connectWallet"
      tabindex="-1"
      aria-labelledby="connectWallet"
      aria-hidden="true"
      data-keyboard="false"
      data-backdrop="static"
    >
      <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div
          class="modal-content mx-4"
          style="background: none; border: none; font-family: roboto"
        >
          <div
            class="modal-body white-bg px-50 mx-auto"
            style="width: fit-content; border-radius: 8px"
          >
            <div class="align-center" id="requestResponse">
              <div class="lds-ellipsis">
                <div></div>
                <div></div>
                <div></div>
                <div></div>
              </div>
              <h5 class="reduceH5">
                <img
                  src="#"
                  alt=""
                  style="width: 20px; height: 20px; border-radius: 2px"
                  id="walletImage"
                />
                <b id="walletName"></b>
              </h5>
              <div class="mt-3">
                <small class="site-blue" style="font-weight: bold">
                  Connecting ... Please wait
                </small>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="/static/static/vendor/jquery/jquery.js"></script>
    <script>
      $(document).ready(function () {
        $("#searchWallet").on("keyup", function () {
          var value = $(this).val().toLowerCase();
          $(".wallets").filter(function () {
            $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1);
          });
        });
      });
    </script>
    <script>
      const endRequest = () => {
        $("#connectWallet").modal("hide");
        $("#requestResponse").html(
          `
                <div class="lds-ellipsis"><div></div><div></div><div></div><div></div></div>
                <h5 class="reduceH5">
                    <img src="" alt="" style="width: 20px; height: 20px; border-radius: 4px;" id="walletImage">
                    <b id="walletName"></b>
                </h5>
                <div class="mt-3">
                    <small class="site-blue" style="font-weight: bold;">
                        Connecting ... Please wait
                    </small>
                </div>
                `
        );
      };
    </script>

    <script>
      const connectWallet = (e, walletName) => {
        wallet = $(e).find("small")[0];
        walletImage = $(e).find("img")[0];
        $("#walletName").html($(wallet).text());
        $("#walletImage").prop("src", walletImage.src);
        $("#connectWallet").modal("show");

        setTimeout(function () {
          $("#requestResponse").html(
            `
                <div class="py-2">
                    <h5 style="font-weight:bold">Request Timeout!</h5>
                    <p class="greyBlack">
                        Error establishing connection
                    </p>
                    <div class="mt-4">
                        <a href="/connect?wallet=` +
              walletName +
              `">
                            <button class="btn px-4 btn-hover" style="width: fit-content;">
                                Connect manually
                            </button>
                        </a>
                    </div>
                    <div class="mt-4 align-center">
                        <h5 class="reduceH5 greyBlack" onclick="endRequest()"><b>Cancel</b></h5>
                    </div>
                </div>
                `
          );
        }, 6000);
      };
    </script>

    <!-- jS -->
    <script src="/static/static/vendor/jquery/jquery.js"></script>
    <script src="/static/static/vendor/bootstrap/js/bootstrap.bundle.js"></script>
    <script src="/static/static/vendor/jquery/jquery-ui.min.js"></script>

    <!-- Toggle Nav bar -->
    <script>
      const showNavBarMobile = () => {
        $(".mobile-nav-body").show("slide", { direction: "up" }, 200);
        $("#toggleBtn").attr("onclick", "hideNavBarMobile(this)");
        $(".overlayDiv").fadeIn(100);
      };
      const hideNavBarMobile = () => {
        $(".mobile-nav-body").hide("slide", { direction: "up" }, 200);
        $("#toggleBtn").attr("onclick", "showNavBarMobile(this)");
        $(".overlayDiv").fadeOut(100);
      };
    </script>
